{
  "build": {
    "content":
      [
        {
          "files": ["**/*.md"],
          "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
        }
      ],
    "resource": [
        {
          "files": ["**/media/**"],
          "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
        }
    ],
    "globalMetadata": {
        "ROBOTS": "INDEX, FOLLOW"
    },
    "externalReference": [
    ],
    "template": "../Templates/HelpServer",
    "dest": "c:/output"
  }
}